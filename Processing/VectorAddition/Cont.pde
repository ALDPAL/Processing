class Cont
{
  Cont(){}
  int x;
  int y;
  
  
  int getContX()
  {
    return x;
  }

  int getContY()
  {
    return y;
  }
  
  
  void setContX(int value)
  {
    x = value;
  }

  void setContY(int value)
  {
    y = value;
  }

 Cont(int x, int y) 
  {
    this.x = x;
    this.y = y;
  }
}